{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Psychological Distress Among Australian Youth (18-24), 2018",
  "width": 800,
  "height": 400,
  "projection": {"type": "mercator"},
  "data": {
    "url": "https://raw.githubusercontent.com/eibi0001/Vega-Lite/main/ne_10m_admin_1_states_provinces.json",
    "format": {"type": "topojson", "feature": "ne_10m_admin_1_states_provinces"}
  },
  "transform": [
    {
      "lookup": "properties.name",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/eibi0001/Vega-Lite/main/Distress%20level%20by%20State.csv"
        },
        "key": "State",
        "fields": ["Percent"]
      }
    }
  ],
  "layer": [
    {
      "mark": {"type": "geoshape"},
      "encoding": {
        "color": {
          "field": "Percent",
          "type": "quantitative",
          "scale": {"type": "linear", "scheme": "reds"}
        },
        "tooltip": [
          {"field": "properties.name", "type": "nominal", "title": "State"},
          {
            "field": "Percent",
            "type": "quantitative",
            "title": "High Psychological Distress %"
          }
        ]
      }
    },
    {
      "mark": {"type": "text", "fontSize": 11, "fontWeight": "normal"},
      "encoding": {
        "text": {"field": "properties.name"},
        "longitude": {"field": "properties.longitude"},
        "latitude": {"field": "properties.latitude"}
      },
      "transform": [
        {
          "filter": "datum.properties.name != 'Australian Capital Territory' && datum.properties.name != 'Ashmore and Cartier Islands' && datum.properties.name != 'Coral Sea Islands' && datum.properties.name != 'Jervis Bay Territory' && datum.properties.name != 'Nusa Tenggara Timur'"
        }
      ]
    }
  ],
  "config": {}
}